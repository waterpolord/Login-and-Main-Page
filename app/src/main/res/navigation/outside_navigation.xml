<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/outside_login"
    tools:ignore="UnusedNavigation">
    <fragment
        android:id="@+id/outside_login"
        android:name="com.proyectoclase.e_comerce.ui.login.loginFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_outside_login_self"
            app:destination="@id/outside_login" />
    </fragment>
    <fragment
        android:id="@+id/nav_home"
        android:name="com.proyectoclase.e_comerce.ui.home.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" />

</navigation>