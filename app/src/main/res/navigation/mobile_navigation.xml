        android:id="@+id/nav_home"
        android:name="com.proyectoclase.e_comerce.ui.home.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/nav_list_os"
